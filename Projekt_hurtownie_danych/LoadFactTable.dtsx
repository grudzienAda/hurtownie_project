                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select cast(medal_id as int) as medal_id, medal_name, cast(c1.date_id as varchar) as date_id, cast(athlete_id as int) as athlete_id, cast(bb.event_id as int) as event_id, cast(loc.location_id as int) as location_id, cast(team.team_id as int) as team_id, Games as games_id,
medal_score

from

(select *, 
row_number() over (order by cast(athlete_id as int)) medal_id,
1 as status 
from 
(select distinct cast(ID as int) as athlete_id, Name as full_name,  Games,
case when Medal='Gold' then 5 else
	case when Medal='Silver' then 3 else
		case when Medal='Bronze' then 1 else 0 end
	end
end as medal_score,
case when Medal='' then 'lack' else Medal end as medal_name,
Year, Season, Sport, Event, City, NOC
from new_athlete_events
where Year&gt;=2000 and Year&lt;=2015
)a
)b

left join date_table as c1
on b.Season = c1.season and b.Year = c1.year

left join (select *,
row_number() over (order by event_name) event_id
from
(select distinct Sport as sport_name, Event as event_name
from new_athlete_events
where Year&gt;=2000 and Year&lt;=2015)aa)bb
on b.Sport=bb.sport_name and b.Event=bb.event_name

left join (
select distinct Continent as continent, Country as country, City as city,
row_number() over (order by country) location_id
from  [list-host-cities-olympic-943j]
where Year&gt;=2000 and Year&lt;=2015) loc
on b.City=loc.city

left join (
select
row_number() over (order by NOC) team_id,
region as team_name, region as team_country, NOC
 from noc_regions)team
 on b.NOC=team.NOC

order by medal_id</property>
